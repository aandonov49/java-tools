<!---->
<!-- DTD2Java settings -->
<!---->
<!--
    RE - Regular Expression
    ML - Max Length
-->
<!---->
<!-- Base types -->
<!---->
<!--
    Boolean
    The values of Boolean are yes/no instead of true/false
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:simpleType name="Boolean">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!--
        Tristate
        The values of Tristate are [YES, NO, UNKNOWN].
    -->
    <xs:simpleType name="Tristate">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!--
        Integer
        32-bit signed integer
    -->
    <xs:simpleType name="Integer">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!--
        Long
        64-bit signed integer
    -->
    <xs:simpleType name="Long">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="Float">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="Double">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="String">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!-- Credit card company code. -->
    <xs:simpleType name="CCCompanyCode">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!--
        DateTime
        MM/dd/yyyy [HH:mm] - RE: ^\d{2}\/\d{2}\/\d{4}( \d{2}:\d{2})?$ - ML: 16
    -->
    <xs:simpleType name="DateTime">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!--
        DateAndTime
        MM/dd/yyyy HH:mm:ss - RE: ^\d{2}\/\d{2}\/\d{4} \d{2}:\d{2}:\d{2}$ - ML: 19
    -->
    <xs:simpleType name="DateAndTime">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!--
        EncryptedNumber
        Signals that the attribute value could be obfuscated in the request
    -->
    <xs:simpleType name="EncryptedNumber">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
