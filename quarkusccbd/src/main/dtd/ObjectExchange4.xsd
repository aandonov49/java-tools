    <!---->
    <!-- Elements -->
    <!---->
    <!--
        ObjectExchange
        version - the version of the interface
    -->
    <xs:element name="ObjectExchange">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="Request"/>
                <xs:element ref="Response"/>
            </xs:choice>
            <xs:attribute name="version" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- Request -->
    <xs:element name="Request">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Recipient"/>
                <xs:element ref="BookingDetails"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!-- Recipient -->
    <xs:element name="Recipient">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="Identifications"/>
                <xs:element ref="Action"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!-- Identifications -->
    <xs:element name="Identifications">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Identification"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        Identification
        name  - name of the identification information  - RE: ^([^<>&%\*\#\s]([^<>&%\*\#]*[^<>&%\*\#\s])?)?$ - ML: 40
        value - value of the identification information - RE: ^([^<>&%\*\#\s]([^<>&%\*\#]*[^<>&%\*\#\s])?)?$ - ML: 256
    -->
    <xs:element name="Identification">
        <xs:complexType>
            <xs:attribute name="name" use="required" type="String"/>
            <xs:attribute name="value" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        Action
        code - the action to be performed
    -->
    <xs:element name="Action">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="EnumAction"/>
            <xs:attribute name="creationDate" type="DateAndTime"/>
        </xs:complexType>
    </xs:element>
    <!-- BookingDetails -->
    <xs:element name="BookingDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Profiles"/>
                <xs:element ref="Trip"/>
                <xs:element minOccurs="0" ref="Alternatives"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!-- Profiles -->
    <xs:element name="Profiles">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Profile"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        Profile
        id             - the id for this profile                           - RE: ^.{1,256}$ - ML: 256
        role           - specifies whether the profile contains booker or traveler information
        greeting       - the booker's or traveler's greeting
        title          - the booker's or traveler's titles                 - ML: 89
        firstName      - the first name of the booker or traveler          - RE: ^[[:alpha:]][[:alpha:]\-\. \']*[[:alpha:]\.\']$ - ML: 40
        middleName     - the middle name of the booker or traveler         - RE: ^[[:alpha:]][[:alpha:]\-\. \']*[[:alpha:]\.\']$ - ML: 40
        lastName       - the last name of the booker or traveler           - RE: ^[[:alpha:]][[:alpha:]\-\. \']*[[:alpha:]\.\']$ - ML: 40
        telephone      - the telephone number of the booker or traveler    - RE: ^[\+0-9 \(\)\/\-]+$ - ML: 40
        fax            - the fax number of the booker or traveler          - RE: ^[\+0-9 \(\)\/\-]+$ - ML: 40
        mobilePhone    - the mobile phone number of the booker or traveler - RE: ^[\+0-9 \(\)\/\-]+$ - ML: 40
        eMail          - the e-mail address of the booker or traveler      - RE: ^[A-Za-z0-9\_\-\.]+\@([A-Za-z0-9]([A-Za-z0-9\-]*[A-Za-z0-9])?\.)+[A-Za-z]{2,5}$ - ML: 128
        employeeNumber - the employeee number of the booker or traveler    - RE: ^.{1,256}$ - ML: 256
        loginName      - the login of the booker or traveler in cytric v7  - RE: ^.{1,256}$ - ML: 256
        GUID           - the global user identified for this profile       - RE: ^.:\d{1,256}$ - ML: 258
    -->
    <xs:element name="Profile">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="CorporateOrganizations"/>
                <xs:element minOccurs="0" ref="FreeDataFields"/>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="String"/>
            <xs:attribute name="role" use="required" type="EnumProfileRole"/>
            <xs:attribute name="greeting" type="EnumGreeting"/>
            <xs:attribute name="title" type="String"/>
            <xs:attribute name="firstName" use="required" type="String"/>
            <xs:attribute name="middleName" type="String"/>
            <xs:attribute name="lastName" use="required" type="String"/>
            <xs:attribute name="gender" type="EnumGender"/>
            <xs:attribute name="telephone" type="String"/>
            <xs:attribute name="fax" type="String"/>
            <xs:attribute name="mobilePhone" type="String"/>
            <xs:attribute name="eMail" use="required" type="String"/>
            <xs:attribute name="employeeNumber" type="String"/>
            <xs:attribute name="loginName" type="String"/>
            <xs:attribute name="nationality" type="String"/>
            <xs:attribute name="GUID" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- CorporateOrganizations -->
    <xs:element name="CorporateOrganizations">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="CorporateOrganization"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        CorporateOrganization
        name  - the name of the organization the booker or traveler belongs to
        value - the value of the organization the booker or traveler belongs to - RE: ^.{1,128}$ - ML: 128
    -->
    <xs:element name="CorporateOrganization">
        <xs:complexType>
            <xs:attribute name="name" use="required" type="EnumCorporateOrganization"/>
            <xs:attribute name="value" use="required" type="String"/>
            <xs:attribute name="id" type="String"/>
            <xs:attribute name="extRef" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- FreeDataFields -->
    <xs:element name="FreeDataFields">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="FreeDataField"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        FreeDataField
        name       - the name of a free data field from the booker's or traveler's profile     - RE: ^.{1,128}$ - ML: 128
        value      - the value of this free data field from the booker's or traveler's profile - RE: ^.{1,256}$ - ML: 256
        costCenter - indicator whether the free data field is a cost center
    -->
    <xs:element name="FreeDataField">
        <xs:complexType>
            <xs:attribute name="name" use="required" type="String"/>
            <xs:attribute name="value" use="required" type="String"/>
            <xs:attribute name="costCenter" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <!--
        ProfileReference
        id - the identifier of the referenced profile, see //Profile/@id
    -->
    <xs:element name="ProfileReference">
        <xs:complexType>
            <xs:attribute name="id" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        Alternatives
        Alternatives to the selected trip, for instance, the unselected Air Options
        will be listed here.
    -->
    <xs:element name="Alternatives">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Trip"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        Trip
        cytricTripNumber      - the trip id within cytric v7
        externalTripNumber    - the trip id within the customer's system                 - RE: ^.{1,40}$ - ML: 40
        cytricRequestCode     - cTW Requiest ID
        creationDateAndTime   - the date and time of the trip creation withing cytric v7
        identification        - the name or identification of the trip                   - RE: ^.{1,256}$ - ML: 256
        globalTripId          - the trip id within cytric v7 of coherent bookings
        purpose               - the reason why the trip is made                          - RE: ^[^<>&%\*\#\?]*$ - ML: 40
        startDateAndTime      - the date and optionally the time when the trip begins
        completionDateAndTime - the date and optionally the time when the trip ends
        approvalStatus        - the actual approval status at the XML creation time
        approvalDueDate       - date and time till when the trip has to be approved
                                respectively rejected
        approvalDateAndTime   - the date and time when the trip was approved.
        approverComment       - a comment by the approver, regarding the resolution
                                 Max length: 256.
        reasonForThisTrip     - a reason, entered by the user for approval purposes
                                 Max length: 256.
        approvalSource        - where the approval was or is to be processed.
        
        lastUpdatedDateAndTime - the date and time of the last change of the trip,
                                 that cytric is aware of.
    -->
    <xs:element name="Trip">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="TripDetails"/>
                <xs:element minOccurs="0" ref="ExpenseData"/>
            </xs:sequence>
            <xs:attribute name="cytricTripNumber" use="required" type="Long"/>
            <xs:attribute name="externalTripNumber" type="String"/>
            <xs:attribute name="cytricRequestCode" type="Long"/>
            <xs:attribute name="creationDateAndTime" use="required" type="DateAndTime"/>
            <xs:attribute name="identification" type="String"/>
            <xs:attribute name="globalTripId" type="Long"/>
            <xs:attribute name="purpose" type="String"/>
            <xs:attribute name="startDateAndTime" use="required" type="DateTime"/>
            <xs:attribute name="completionDateAndTime" type="DateTime"/>
            <xs:attribute name="approvalStatus" type="EnumApprovalStatus"/>
            <xs:attribute name="approvalDueDate" type="DateAndTime"/>
            <xs:attribute name="approvalDateAndTime" type="DateAndTime"/>
            <xs:attribute name="approverComment" type="String"/>
            <xs:attribute name="reasonForThisTrip" type="String"/>
            <xs:attribute name="approvalSource" type="EnumApprovalSource"/>
            <xs:attribute name="lastUpdatedDateAndTime" type="DateAndTime"/>
            <xs:attribute name="approvalRejectReasonCode" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- TripDetails -->
    <xs:element name="TripDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="TripDetail"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        ExpenseData
        destination - the destination from cTW expense data page
        destinationCountryCode - the country code from cTW expense data page
        privUntilDate - the "Personal Time/until" date from cTW expense data page
        privFromDate - the "Personal Time/from" date from cTW expense data page
        untilDate - the "Business Travel/until" date from cTW expense data page
        fromDate - the "Business Travel/from" date from cTW expense data page
    -->
    <xs:element name="ExpenseData">
        <xs:complexType>
            <xs:attribute name="destination" type="String"/>
            <xs:attribute name="destinationCountryCode" type="String"/>
            <xs:attribute name="privUntilDate" type="DateTime"/>
            <xs:attribute name="privFromDate" type="DateTime"/>
            <xs:attribute name="untilDate" type="DateTime"/>
            <xs:attribute name="fromDate" type="DateTime"/>
        </xs:complexType>
    </xs:element>
    <!--
        TripDetail
        cytricTripDetailNumber     - the trip details id within cytric v7
        cytricRequestDetailNumber  - cTW Request Segment Id
        type                       - specifies the type of booking
        canceled                   - the trip is canceled
        bookingDateAndTime         - the date and time the booking has been made
        startDateAndTime           - the date and optionally the time when the service booked begins
        completionDateAndTime      - the date and optionally the time when the service booked ends
        duration                   - the time difference between completionDateAndTime and startDateAndTime        - RE: ^(\d{1,3}D\d{1,2}H\d{1,2}M)|(\d{1,3}N)$ - ML: 10
        origin                     - the origin of the booking (city, airport, railway or car pick-up station)     - RE: ^.{1,1024}$ - ML: 1024
        destination                - the destination of the booking (city, airport, railway or car return station) - RE: ^.{1,1024}$ - ML: 1024
        originIATACode             - the IATA code of the origin
        originIATAName             - the IATA name of the origin                 - RE: ^.{1,1024}$ - ML: 1024
        destinationIATACode        - the IATA code of the destination
        destinationIATAName        - the IATA name of the destination            - RE: ^.{1,1024}$ - ML: 1024
        originCountryCode          - the origin country code of the booking      - RE: ^[A-Z]{2}$ - ML: 2
        originCountry              - the origin country name of the booking      - RE: ^.{1,1024}$ - ML: 1024
        destinationCountryCode     - the destination country code of the booking - RE: ^[A-Z]{2}$ - ML: 2
        destinationCountry         - the destination country name of the booking - RE: ^.{1,1024}$ - ML: 1024
        originArea                 - the origin area of the booking
        destinationArea            - the destination area of the booking
        inventorySystem            - the booking or inventory system accessed for the booking
        inventoryRecordLocator     - the record locator in the inventory system  - RE: ^.{1,64}$  - ML: 64
        passiveSystem              - the booking system where the passive PNR was created
        passiveType                - the type of the passive booking
        passiveRecordLocator       - the record locator of the passive booking in the booking system - RE: ^.{1,64}$  - ML: 64
        onHold                     - an on-hold booking status indicator
        bookingSource              - indicates whether the booking was initially created online or offline
        fareLogixLocator           - DynaWeb Farelogix PNR
    -->
    <xs:element name="TripDetail">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="Payments"/>
                <xs:element minOccurs="0" ref="Prices"/>
                <xs:choice>
                    <xs:element ref="TripDetailsAir"/>
                    <xs:element ref="TripDetailsHotel"/>
                    <xs:element ref="TripDetailsEvent"/>
                    <xs:element ref="TripDetailsRail"/>
                    <xs:element ref="TripDetailsCar"/>
                    <xs:element ref="TripDetailsCloud"/>
                    <xs:element ref="TripDetailsWizzard"/>
                </xs:choice>
                <xs:element minOccurs="0" ref="TravelPolicyAlert"/>
                <xs:element minOccurs="0" ref="FreeDataFields"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ProfileReference"/>
            </xs:sequence>
            <xs:attribute name="cytricTripDetailNumber" use="required" type="Long"/>
            <xs:attribute name="cytricRequestDetailNumber" type="Long"/>
            <xs:attribute name="type" use="required" type="EnumTripDetailType"/>
            <xs:attribute name="canceled" type="Boolean"/>
            <xs:attribute name="bookingDateAndTime" use="required" type="DateAndTime"/>
            <xs:attribute name="startDateAndTime" use="required" type="DateTime"/>
            <xs:attribute name="completionDateAndTime" type="DateTime"/>
            <xs:attribute name="duration" type="String"/>
            <xs:attribute name="origin" use="required" type="String"/>
            <xs:attribute name="destination" use="required" type="String"/>
            <xs:attribute name="originIATACode" type="String"/>
            <xs:attribute name="originIATAName" type="String"/>
            <xs:attribute name="destinationIATACode" type="String"/>
            <xs:attribute name="destinationIATAName" type="String"/>
            <xs:attribute name="originCountryCode" type="String"/>
            <xs:attribute name="originCountry" type="String"/>
            <xs:attribute name="destinationCountryCode" type="String"/>
            <xs:attribute name="destinationCountry" type="String"/>
            <xs:attribute name="originArea" type="EnumArea"/>
            <xs:attribute name="destinationArea" type="EnumArea"/>
            <xs:attribute name="inventorySystem" type="EnumInventorySystem"/>
            <xs:attribute name="inventoryRecordLocator" type="String"/>
            <xs:attribute name="passiveSystem" type="EnumPassiveSystem"/>
            <xs:attribute name="passiveType" type="EnumPassiveType"/>
            <xs:attribute name="passiveRecordLocator" type="String"/>
            <xs:attribute name="onHold" type="Boolean"/>
            <xs:attribute name="delayedTicketing" type="Boolean"/>
            <xs:attribute name="bookingSource" type="EnumBookingSource"/>
            <xs:attribute name="fareLogixLocator" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- Payments -->
    <xs:element name="Payments">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Payment"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        Payment
        type          - the type of payment used
        profileId     - the profile to which this payment refers    - RE: ^.{1,256}$ - ML: 256
    -->
    <xs:element name="Payment">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="CreditCard"/>
                <xs:element minOccurs="0" ref="OriginalCreditCard"/>
            </xs:sequence>
            <xs:attribute name="type" use="required" type="EnumPaymentType"/>
            <xs:attribute name="profileId" use="required" type="String"/>
            <xs:attribute name="paidByCompany" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <!--
        CreditCard
        companyCode   - the credit card company code
        number        - the credit card number       - RE: ^\d{1,20}$ - ML: 20
        paidByCompany - specifies whether the booking is paid by the company
    -->
    <xs:element name="CreditCard">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="CreditCardField"/>
            </xs:sequence>
            <xs:attribute name="companyCode" use="required" type="CCCompanyCode"/>
            <xs:attribute name="number" use="required" type="EncryptedNumber"/>
            <xs:attribute name="cvn" type="EncryptedNumber"/>
            <xs:attribute name="paidByCompany" type="Boolean"/>
            <xs:attribute name="expireDate" type="DateTime"/>
            <xs:attribute name="corporateCard" type="Boolean"/>
            <xs:attribute name="cardHolder" type="String"/>
            <xs:attribute name="cardIsPresentWithTraveller" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <!--
        OriginalCreditCard
        companyCode   - the credit card company code
        number        - the credit card number       - RE: ^\d{1,20}$ - ML: 20
        paidByCompany - specifies whether the booking is paid by the company
    -->
    <xs:element name="OriginalCreditCard">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="CreditCardField"/>
            </xs:sequence>
            <xs:attribute name="companyCode" use="required" type="CCCompanyCode"/>
            <xs:attribute name="number" use="required" type="EncryptedNumber"/>
            <xs:attribute name="accountNumber" type="String"/>
            <xs:attribute name="cvn" type="EncryptedNumber"/>
            <xs:attribute name="paidByCompany" type="Boolean"/>
            <xs:attribute name="expireDate" type="DateTime"/>
            <xs:attribute name="corporateCard" type="Boolean"/>
            <xs:attribute name="cardHolder" type="String"/>
            <xs:attribute name="cardIsPresentWithTraveller" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="CreditCardField">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="String"/>
            <xs:attribute name="value" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        Prices
        type                  - the prices type, i.e. total, daily, weekend, etc.
        source                - the fare source type, i.e. published, negotiated, private or corporate
    -->
    <xs:element name="Prices">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Price"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="AirPrices"/>
            </xs:sequence>
            <xs:attribute name="type" use="required" type="EnumPricesType"/>
            <xs:attribute name="source" type="EnumFareSource"/>
        </xs:complexType>
    </xs:element>
    <!--
        A Pricing Group (TST)
          airSubSegmentIds      - a list of segment ID-s (in NMTOKENS format), see AirSubsegment/@id. No limit is imposed on the maximal length.
          source                - the fare source type, i.e. published, negotiated, private or corporate
          dynamicDiscountedFare - dynamic discounted fare (yes/no)?
          spanishResidentFare   - dynamic discounted fare (yes/no)?
          tourCode              - tour code              - RE: ^.{1,50}$ - ML: 50
    -->
    <xs:element name="AirPrices">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Price"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Reduction"/>
            </xs:sequence>
            <xs:attribute name="airSubSegmentIds" type="String"/>
            <xs:attribute name="source" type="EnumFareSource"/>
            <xs:attribute name="dynamicDiscountedFare" default="no" type="Boolean"/>
            <xs:attribute name="spanishResidentFare" default="no" type="Boolean"/>
            <xs:attribute name="tourCode" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        Price
        profile id - the profile to which this price refers
        type     - the type of the amount
        source   - the price source type, i.e. provider, first destination or company
        currency - the currency relating to the amount - RE: ^[A-Z]{3}$ - ML: 3
        value    - the amount                          - RE: ^[\-+]\d{8}\.\d{2}?$ - ML: 12
        providerFeeCode - fee code in case
        feeDescription  - fee description
    -->
    <xs:element name="Price">
        <xs:complexType>
            <xs:attribute name="profileId" type="String"/>
            <xs:attribute name="type" type="EnumPriceType"/>
            <xs:attribute name="source" type="EnumPriceSource"/>
            <xs:attribute name="currency" type="String"/>
            <xs:attribute name="value" type="Float"/>
            <xs:attribute name="providerFeeCode" type="String"/>
            <xs:attribute name="feeDescription" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        Reduction
        type  - the type of the reduction
        value - the value or code of the reduction     - RE: ^.{1,50}$ - ML: 50
    -->
    <xs:element name="Reduction">
        <xs:complexType>
            <xs:attribute name="type" use="required" type="EnumReductionType"/>
            <xs:attribute name="value" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        TripDetailsAir
        ticketingDate   - the ticket issue date.
        ticketIssued    - indicates if the ticket has been issued.
        isOption        - identifies this as an Air Option.
    -->
    <xs:element name="TripDetailsAir">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="AirSubsegments"/>
                <xs:element minOccurs="0" ref="HighestFare"/>
                <xs:element minOccurs="0" ref="LowestFare"/>
            </xs:sequence>
            <xs:attribute name="ticketingDate" type="DateTime"/>
            <xs:attribute name="ticketIssued" type="Tristate"/>
            <xs:attribute name="ticketType" type="EnumAirTicketType"/>
            <xs:attribute name="isOption" type="Boolean"/>
            <xs:attribute name="preferred" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <!-- AirSubsegments -->
    <xs:element name="AirSubsegments">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="AirSubsegment"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        AirSubsegment
        id                         - the segment identifier is valid only for the current document.
        direction                  - the direction of the segment
        airlineCode                - the airline code of the air segment
        airline                    - the airline name of the air segment            - RE: ^.{1,1024}$ - ML: 1024
        operatedByAirlineCode      - the operating airline code of the air segment
        operatedByAirlineCode      - the operating airline name of the air segment
        inventoryCode              - the flight number of the air segment           - RE: ^\d{1,256}$ - ML: 256
        origin                     - the origin of the air segment                  - RE: ^.{1,1024}$ - ML: 1024
        destination                - the destination of the air segment             - RE: ^.{1,1024}$ - ML: 1024
        originTerminal             - the terminal of the origin airport             - RE: ^.{1,128}$ - ML: 128
        destinationTerminal        - the terminal of the destination airport        - RE: ^.{1,128}$ - ML: 128
        originDateAndTime          - the date and optionally the time when the flight departs
        destinationDateAndTime     - the date and optionally the time when the flight arrives
        originCityCode             - the origin city code
        originCity                 - the origin city name                           - RE: ^.{1,1024}$ - ML: 1024
        destinationCityCode        - the destination city code
        destinationCity            - the destination city name                      - RE: ^.{1,1024}$ - ML: 1024
        originCountryCode          - the origin country code of the air segment
        originCountry              - the origin country name of the air segment     - RE: ^.{1,1024}$ - ML: 1024
        destinationCountryCode     - the destination country code of the air segment
        destinationCountry         - the destination country name of the air segment    - RE: ^.{1,1024}$ - ML: 1024
        originIATACode             - the IATA code of the origin
        destinationIATACode        - the IATA code of the destination
        originArea                 - the origin area of the air segment
        destinationArea            - the destination area of the air segment
        bookingClass               - the booking class booked for the flight
        serviceClass               - the service class booked for the flight
        providerConfirmationNumber - the confirmation number under which the booking is made at the provider - RE: ^.{1,16}$ - ML: 16
        fareBasis                  - the fare basis                                 - RE: ^.{1,64}$   - ML: 64
        equipment                  - the aircraft type                              - RE: ^.{1,1024}$ - ML: 1024
        codeShared                 - the code-shared flight indicator
    -->
    <xs:element name="AirSubsegment">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Tickets"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="FlightBaggage"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="FlightSeat"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="FlightMeal"/>
                <xs:element minOccurs="0" ref="CarbonEmission"/>
                <xs:element minOccurs="0" ref="AirlineLogos"/>
                <xs:element minOccurs="0" ref="OperatedByAirlineLogos"/>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="Integer"/>
            <xs:attribute name="direction" type="Integer"/>
            <xs:attribute name="airlineCode" type="String"/>
            <xs:attribute name="airline" type="String"/>
            <xs:attribute name="operatedByAirlineCode" type="String"/>
            <xs:attribute name="operatedByAirline" type="String"/>
            <xs:attribute name="inventoryCode" type="String"/>
            <xs:attribute name="origin" type="String"/>
            <xs:attribute name="destination" type="String"/>
            <xs:attribute name="originTerminal" type="String"/>
            <xs:attribute name="destinationTerminal" type="String"/>
            <xs:attribute name="originDateAndTime" type="DateTime"/>
            <xs:attribute name="destinationDateAndTime" type="DateTime"/>
            <xs:attribute name="originCityCode" type="String"/>
            <xs:attribute name="originCity" type="String"/>
            <xs:attribute name="destinationCityCode" type="String"/>
            <xs:attribute name="destinationCity" type="String"/>
            <xs:attribute name="originCountryCode" type="String"/>
            <xs:attribute name="originCountry" type="String"/>
            <xs:attribute name="destinationCountryCode" type="String"/>
            <xs:attribute name="destinationCountry" type="String"/>
            <xs:attribute name="originIATACode" type="String"/>
            <xs:attribute name="destinationIATACode" type="String"/>
            <xs:attribute name="originArea" type="EnumArea"/>
            <xs:attribute name="destinationArea" type="EnumArea"/>
            <xs:attribute name="bookingClass" type="EnumBookingClass"/>
            <xs:attribute name="serviceClass" type="EnumAirServiceClass"/>
            <xs:attribute name="providerConfirmationNumber" type="String"/>
            <xs:attribute name="fareBasis" type="String"/>
            <xs:attribute name="equipment" type="String"/>
            <xs:attribute name="codeShared" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="AirlineLogos">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Logo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <xs:element name="OperatedByAirlineLogos">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Logo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <xs:element name="FlightSeat">
        <xs:complexType>
            <xs:attribute name="profileId" type="String"/>
            <xs:attribute name="number" type="String"/>
            <xs:attribute name="confirmed" type="Boolean"/>
            <xs:attribute name="chargeable" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="FlightBaggage">
        <xs:complexType>
            <xs:attribute name="profileId" type="String"/>
            <xs:attribute name="description" type="String"/>
            <xs:attribute name="weight" type="String"/>
            <xs:attribute name="unit" type="String"/>
            <xs:attribute name="pieces" type="String"/>
            <xs:attribute name="chargeable" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="FlightMeal">
        <xs:complexType>
            <xs:attribute name="profileId" type="String"/>
            <xs:attribute name="description" type="String"/>
            <xs:attribute name="confirmed" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="Tickets">
        <xs:complexType>
            <xs:attribute name="number" type="String"/>
            <xs:attribute name="profileId" use="required" type="String"/>
            <xs:attribute name="freeText" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- HighestFare -->
    <xs:element name="HighestFare">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Price"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- No attribute -->
    <!-- LowestFare -->
    <xs:element name="LowestFare">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Price"/>
            </xs:sequence>
            <xs:attribute name="fareType" type="EnumFareType"/>
        </xs:complexType>
    </xs:element>
    <!--
        LowestFare
        fareType  - the type of the lowest fare
    -->
    <xs:element name="LowestFares">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="LowestFare"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- No attribute -->
    <!--
        TripDetailsHotel
        inventoryCode              - the cytric hotel ID of the booked hotel - RE: ^.{1,20}$ - ML: 20
        name                       - the name of the hotel                   - RE: ^.{1,64}$ - ML: 64
        street                     - the steet of the hotel address          - RE: ^.{1,128}$ - ML: 128
        city                       - the city of the hotel address           - RE: ^.{1,96}$ - ML: 96
        country                    - the country of the hotel address        - RE: ^.{1,64}$ - ML: 64
        telephone                  - the telephone number of the hotel       - RE: ^.{1,32}$ - ML: 32
        hotelChain                 - the hotel chain                         - RE: ^[A-Z0-9]{2}$ - ML: 2
        providerConfirmationNumber - the confirmation number under which the booking is made at the provider - RE: ^.{1,64}$ - ML: 64
        stars                      - the number of stars of the booked hotel
        cancellationPolicy         - the cancellation policy of the booked room
        statusCode                 - the status code of the hotel confirmation
        planCode                   - the rate plan for this rate
        roomCode                   - the room type for this rate
    -->
    <xs:element name="TripDetailsHotel">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="HotelRate"/>
                <xs:element minOccurs="0" ref="GeoLocation"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Image"/>
                <xs:element minOccurs="0" ref="ChainLogos"/>
                <xs:element minOccurs="0" ref="ProviderLogos"/>
                <xs:element minOccurs="0" ref="LowestFares"/>
            </xs:sequence>
            <xs:attribute name="inventoryCode" type="String"/>
            <xs:attribute name="name" type="String"/>
            <xs:attribute name="street" type="String"/>
            <xs:attribute name="city" type="String"/>
            <xs:attribute name="country" type="String"/>
            <xs:attribute name="telephone" type="String"/>
            <xs:attribute name="hotelChain" type="String"/>
            <xs:attribute name="breakfastIncluded" use="required" type="Tristate"/>
            <xs:attribute name="roomOccupancy" use="required" type="Integer"/>
            <xs:attribute name="providerConfirmationNumber" type="String"/>
            <xs:attribute name="stars" type="Integer"/>
            <xs:attribute name="cancellationPolicy" type="String"/>
            <xs:attribute name="preferred" type="Boolean"/>
            <xs:attribute name="isPartOfProgram" type="Boolean"/>
            <xs:attribute name="statusCode" type="String"/>
            <xs:attribute name="planCode" type="String"/>
            <xs:attribute name="roomCode" type="String"/>
            <xs:attribute name="fax" type="String"/>
            <xs:attribute name="postalcode" type="String"/>
            <xs:attribute name="email" type="String"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ChainLogos">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Logo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <xs:element name="ProviderLogos">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Logo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!-- HotelRate -->
    <xs:element name="HotelRate">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="HotelRateInfo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!--
        HotelRateInfo
        category -
        content  -
    -->
    <xs:element name="HotelRateInfo">
        <xs:complexType>
            <xs:attribute name="category" type="String"/>
            <xs:attribute name="content" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        TripDetailsEvent
        inventoryCode              - the cytric hotel ID of the booked hotel - RE: ^.{1,20}$ - ML: 20
        name                       - event name                              - RE: ^.{1,64}$ - ML: 255
        hotelname                  - the name of the hotel                   - RE: ^.{1,64}$ - ML: 64
        street                     - the steet of the hotel address          - RE: ^.{1,128}$ - ML: 128
        city                       - the city of the hotel address           - RE: ^.{1,96}$ - ML: 96
        country                    - the country of the hotel address        - RE: ^.{1,64}$ - ML: 64
        telephone                  - the telephone number of the hotel       - RE: ^.{1,32}$ - ML: 32
        hotelChain                 - the hotel chain                         - RE: ^[A-Z0-9]{2}$ - ML: 2
        stars                      - the number of stars of the booked hotel
        cancellationAllowedUntil   - the date until cancellation is possible
        cancellationPolicy         - the cancellation policy of the booked room
    -->
    <xs:element name="TripDetailsEvent">
        <xs:complexType>
            <xs:attribute name="inventoryCode" type="String"/>
            <xs:attribute name="name" use="required" type="String"/>
            <xs:attribute name="hotelName" type="String"/>
            <xs:attribute name="street" type="String"/>
            <xs:attribute name="city" type="String"/>
            <xs:attribute name="country" type="String"/>
            <xs:attribute name="telephone" type="String"/>
            <xs:attribute name="hotelChain" type="String"/>
            <xs:attribute name="stars" type="Integer"/>
            <xs:attribute name="contact" type="String"/>
            <xs:attribute name="cancellationAllowedUntil" type="DateTime"/>
            <xs:attribute name="cancellationPolicy" type="String"/>
            <xs:attribute name="fax" type="String"/>
            <xs:attribute name="postalcode" type="String"/>
            <xs:attribute name="email" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        TripDetailsRail
        railCard - the rail card used
    -->
    <xs:element name="TripDetailsRail">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="RailCards"/>
                <xs:element ref="RailSubsegments"/>
            </xs:sequence>
            <xs:attribute name="providerConfirmationNumber" type="String"/>
            <xs:attribute name="preferred" type="Boolean"/>
            <xs:attribute name="ticketType" type="EnumRailTicketType"/>
        </xs:complexType>
    </xs:element>
    <!-- RailCards -->
    <xs:element name="RailCards">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="RailCard"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attribute -->
    <!--
        RailCard
        type      - the type of rail card used
        profileId - the profile to which this rail card refers  - RE: ^.{1,256}$ - ML: 256
    -->
    <xs:element name="RailCard">
        <xs:complexType>
            <xs:attribute name="type" use="required" type="String"/>
            <xs:attribute name="profileId" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- RailSubsegments -->
    <xs:element name="RailSubsegments">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="RailSubsegment"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        RailSubsegment
        id                         - the segment identifier is valid only for the current document.
        direction                  - the direction of the segment
        origin                     - the origin station of the rail segment                  - RE: ^.{1,1024}$ - ML: 1024
        destination                - the destination station of the rail segment             - RE: ^.{1,1024}$ - ML: 1024
        originTrack                - the departure (origin) track
        destinationTrack           - the arrival (destination) track
        originCountryCode          - the origin country code of the rail segment
        destinationCountryCode     - the destination country code of the rail segment
        originArea                 - the origin area of the rail segment
        destinationArea            - the destination area of the rail segment
        originDateAndTime          - the date and optionally the time when the train departs
        destinationDateAndTime     - the date and optionally the time when the train arrives
        originIATACode             - the IATA code of the origin
        destinationIATACode        - the IATA code of the destination
        serviceClass               - the service class booked on the train
        providerConfirmationNumber - the confirmation number under which the booking is made at the provider
        trainNumber                - the number of the train
        totalPrice                 - the total price of the segment
        railCompanyName            - the rail company name
        railCompanyCode            - the rail company code
        vehicle                    - the train type, e.g. ICE, MET
        vehicleDescription         - long name of the train type, e.g. Intercity Express or Thalys
    -->
    <xs:element name="RailSubsegment">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="RailSeat"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="GeoLocation"/>
                <xs:element minOccurs="0" ref="CarbonEmission"/>
                <xs:element minOccurs="0" ref="RailCompanyLogos"/>
            </xs:sequence>
            <xs:attribute name="id" type="Integer"/>
            <xs:attribute name="direction" type="Integer"/>
            <xs:attribute name="origin" type="String"/>
            <xs:attribute name="destination" type="String"/>
            <xs:attribute name="originTrack" type="String"/>
            <xs:attribute name="destinationTrack" type="String"/>
            <xs:attribute name="originCountryCode" type="String"/>
            <xs:attribute name="destinationCountryCode" type="String"/>
            <xs:attribute name="originArea" type="EnumArea"/>
            <xs:attribute name="destinationArea" type="EnumArea"/>
            <xs:attribute name="originDateAndTime" type="DateTime"/>
            <xs:attribute name="destinationDateAndTime" type="DateTime"/>
            <xs:attribute name="originIATACode" type="String"/>
            <xs:attribute name="destinationIATACode" type="String"/>
            <xs:attribute name="originCityIATACode" type="String"/>
            <xs:attribute name="destinationCityIATACode" type="String"/>
            <xs:attribute name="originCityName" type="String"/>
            <xs:attribute name="destinationCityName" type="String"/>
            <xs:attribute name="serviceClass" type="EnumRailServiceClass"/>
            <xs:attribute name="providerConfirmationNumber" type="String"/>
            <xs:attribute name="trainNumber" type="String"/>
            <xs:attribute name="totalPrice" type="Float"/>
            <xs:attribute name="railCompanyName" type="String"/>
            <xs:attribute name="railCompanyCode" type="String"/>
            <xs:attribute name="vehicle" type="String"/>
            <xs:attribute name="vehicleDescription" type="String"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="RailCompanyLogos">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Logo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        RailSeat
        profileId    -
        seatNumber   -
        waggonNumber -
    -->
    <xs:element name="RailSeat">
        <xs:complexType>
            <xs:attribute name="profileId" use="required" type="String"/>
            <xs:attribute name="seatNumber" type="String"/>
            <xs:attribute name="waggonNumber" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        TripDetailsCar
        rentalCarCompany           - the name of the rental car company                           - RE: ^.{1,1024}$ - ML: 1024
        inventoryCode              - the acriss code of the booked rental car segment (e.g. ECMN) - RE: ^[A-Z\*]{4}$ - ML: 4
        rentAtAirport              - whether the car pick-up station is at an airport
        telephonePickUpStation     - the telephone number of the pick-up station                  - RE: ^.{1,64}$ - ML: 64
        telephoneReturnStation     - the telephone number of the return station                   - RE: ^.{1,64}$ - ML: 64
        telefaxPickUpStation       - the telefax number of the pick-up station                    - RE: ^.{1,64}$ - ML: 64
        telefaxReturnStation       - the telefax number of the return station                     - RE: ^.{1,64}$ - ML: 64
        emailPickUpStation         - the email of the pick-up station                                - RE: ^[A-Za-z0-9\_\-\.]+\@([A-Za-z0-9]([A-Za-z0-9\-]*[A-Za-z0-9])?\.)+[A-Za-z]{2,5}$ - ML: 128
        emailReturnStation            - the email of the return station                                 - RE: ^[A-Za-z0-9\_\-\.]+\@([A-Za-z0-9]([A-Za-z0-9\-]*[A-Za-z0-9])?\.)+[A-Za-z]{2,5}$ - ML: 128
        providerConfirmationNumber - the confirmation number under which the booking is made at the provider - RE: ^.{1,16}$ - ML: 16
        statusCode                 - the status code of the car confirmation
        rateCode                   - the rate code of the car rental
    -->
    <xs:element name="TripDetailsCar">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="CarCategory"/>
                <xs:element minOccurs="0" ref="CarType"/>
                <xs:element minOccurs="0" ref="CarTransmission"/>
                <xs:element minOccurs="0" ref="CarMiscellaneous"/>
                <xs:element minOccurs="0" ref="CarModel"/>
                <xs:element minOccurs="0" ref="CarFees"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="GeoLocation"/>
                <xs:element minOccurs="0" ref="CarCompanyLogos"/>
            </xs:sequence>
            <xs:attribute name="rentalCarCompany" type="String"/>
            <xs:attribute name="inventoryCode" type="String"/>
            <xs:attribute name="rentAtAirport" type="Boolean"/>
            <xs:attribute name="telephonePickUpStation" type="String"/>
            <xs:attribute name="telephoneReturnStation" type="String"/>
            <xs:attribute name="telefaxPickUpStation" type="String"/>
            <xs:attribute name="telefaxReturnStation" type="String"/>
            <xs:attribute name="emailPickUpStation" type="String"/>
            <xs:attribute name="emailReturnStation" type="String"/>
            <xs:attribute name="providerConfirmationNumber" type="String"/>
            <xs:attribute name="preferred" type="Boolean"/>
            <xs:attribute name="statusCode" type="String"/>
            <xs:attribute name="rateCode" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        CarCategory - the first particle of the ACRISS car classification code, a.k.a. car class
        code         - the car category code
        description  - textual description
    -->
    <xs:element name="CarCategory">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="EnumCarCategory"/>
            <xs:attribute name="description" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        CarType    - the second particle of the ACRISS car classification code
        code        - the car type code
        description - textual description
    -->
    <xs:element name="CarType">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="EnumCarType"/>
            <xs:attribute name="description" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        CarTransmission - the third particle of the ACRISS car classification code
        code             - the car transmission code
        description      - textual description
    -->
    <xs:element name="CarTransmission">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="EnumCarTransmission"/>
            <xs:attribute name="description" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        CarMiscellaneous - traditionally Air Conditioning, the fourth particle of the ACRISS car classification code
        code              - the car air conditioning characteristics code
        description       - textual description
    -->
    <xs:element name="CarMiscellaneous">
        <xs:complexType>
            <xs:attribute name="code" use="required" type="EnumCarMiscellaneous"/>
            <xs:attribute name="description" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        CarModel    - name and description of the car model
        name         - name of the car model
        description  - textual description
    -->
    <xs:element name="CarModel">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" ref="Image"/>
            </xs:sequence>
            <xs:attribute name="name" type="String"/>
            <xs:attribute name="description" type="String"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="CarCompanyLogos">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Logo"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <xs:element name="Logo">
        <xs:complexType>
            <xs:attribute name="url" use="required" type="String"/>
            <xs:attribute name="size" use="required" type="EnumLogoSize"/>
            <xs:attribute name="type" use="required" type="EnumLogoType"/>
            <xs:attribute name="width" type="Integer"/>
            <xs:attribute name="height" type="Integer"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="Image">
        <xs:complexType>
            <xs:attribute name="url" use="required" type="String"/>
            <xs:attribute name="type" type="EnumImageType"/>
            <xs:attribute name="title" type="String"/>
            <xs:attribute name="width" type="Integer"/>
            <xs:attribute name="height" type="Integer"/>
        </xs:complexType>
    </xs:element>
    <!-- Payments -->
    <xs:element name="CarFees">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="CarFee"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        CarFee - fees for car
        type         - the car fee type
    -->
    <xs:element name="CarFee">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Price"/>
            </xs:sequence>
            <xs:attribute name="type" use="required" type="EnumCarFeeType"/>
        </xs:complexType>
    </xs:element>
    <!--
        TripDetailsCloud
        providerName     - the name of the booked appCloud provider
        providerCode     - the cbs code of this provider
        applicationName  - the name of the booked appCloud application.
        applicationCode  - the cbs code of this application
        serviceName      - the name of the booked appCloud service
        rateName         - the name of the booked appCloud rate
        confirmationDate - the date/time when the booked service was confirmed by the provider
    -->
    <xs:element name="TripDetailsCloud">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="Requisite"/>
            </xs:sequence>
            <xs:attribute name="providerName" use="required" type="String"/>
            <xs:attribute name="providerCode" type="String"/>
            <xs:attribute name="applicationName" use="required" type="String"/>
            <xs:attribute name="applicationCode" type="String"/>
            <xs:attribute name="serviceName" type="String"/>
            <xs:attribute name="rateName" type="String"/>
            <xs:attribute name="confirmationDate" type="DateTime"/>
        </xs:complexType>
    </xs:element>
    <!--
        Requisite
        name     - the requisite name
        code     - the requisite code
        value    - the value presented as string
    -->
    <xs:element name="Requisite">
        <xs:complexType>
            <xs:attribute name="name" use="required" type="String"/>
            <xs:attribute name="code" use="required" type="String"/>
            <xs:attribute name="value" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        For CTW Requests , TripDetailsWizzard will be sent in cCBD XML
        and for each OtherTravel Segment , TripDetailsWizzardOther to be sent to cCBD
        Only CTW other Travel Requests are sent to cCBD as of now
    -->
    <xs:element name="TripDetailsWizzard">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="TripDetailsWizzardOther"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <xs:element name="TripDetailsWizzardOther">
        <xs:complexType>
            <xs:attribute name="ctwRequestSegment" use="required" type="Long"/>
            <xs:attribute name="origin" type="String"/>
            <xs:attribute name="destination" type="String"/>
            <xs:attribute name="prefStartDateTime" type="DateTime"/>
            <xs:attribute name="prefEndDateTime" type="DateTime"/>
            <xs:attribute name="remark" type="String"/>
            <xs:attribute name="otherTravelCategory" type="String"/>
            <xs:attribute name="supplierName" type="String"/>
            <xs:attribute name="currencyCode" type="String"/>
            <xs:attribute name="estimatedPrice" type="Float"/>
            <xs:attribute name="canceled" type="Boolean"/>
        </xs:complexType>
    </xs:element>
    <!--
        TravelPolicyAlert
        reasonCode        - the user's explanation why travel policy has been violated, in terms of assigned reason code - ML: 64
        explanation       - the user's free-form explanation why travel policy has been violated - ML: 1024
        reasonCodeComment - Reason code free-text explanation - ML:1024
    -->
    <xs:element name="TravelPolicyAlert">
        <xs:complexType>
            <xs:attribute name="reasonCode" type="String"/>
            <xs:attribute name="explanation" type="String"/>
            <xs:attribute name="reasonCodeComment" type="String"/>
            <xs:attribute name="advanceReasonCode" type="String"/>
            <xs:attribute name="noHotelReasonCode" type="String"/>
            <xs:attribute name="noHotelExplanation" type="String"/>
        </xs:complexType>
    </xs:element>
    <!-- Response -->
    <xs:element name="Response">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="Error"/>
                <xs:element ref="Confirmation"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <!-- no attributes -->
    <!--
        Error
        code        - the error code
        description - the description of the error
    -->
    <xs:element name="Error">
        <xs:complexType>
            <xs:attribute name="code" type="Integer"/>
            <xs:attribute name="description" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        Confirmation
        externalTripNumber - the trip id within the customer's system - RE: ^.{1,40}$ - ML: 40
    -->
    <xs:element name="Confirmation">
        <xs:complexType>
            <xs:attribute name="externalTripNumber" type="String"/>
        </xs:complexType>
    </xs:element>
    <!--
        GeoLocation
        longitude - geographical latitude
        latitude  - geographical longitude
    -->
    <xs:element name="GeoLocation">
        <xs:complexType>
            <xs:attribute name="name" type="String"/>
            <xs:attribute name="longitude" use="required" type="Double"/>
            <xs:attribute name="latitude" use="required" type="Double"/>
        </xs:complexType>
    </xs:element>
    <!--
        CarbonEmission
        amount - ammount of emissions
        unit   - unit of measurement
    -->
    <xs:element name="CarbonEmission">
        <xs:complexType>
            <xs:attribute name="amount" use="required" type="Float"/>
            <xs:attribute name="unit" use="required" type="String"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
